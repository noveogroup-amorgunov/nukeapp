    "rc-dropdown": "4.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.52.1",
    "react-redux": "9.1.2",
    "react-router-dom": "6.24.1",
    "redux-persist": "6.0.0",
    "zod": "3.23.8"
