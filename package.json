    "eslint-plugin-storybook": "0.12.0",
    "globals": "15.8.0",
    "lint-staged": "12.3.2",
    "postcss": "8.4.39",
    "postcss-custom-media": "10.0.8",
    "postcss-import": "16.1.0",
    "postcss-mixins": "10.0.1",
    "postcss-nested": "6.0.1",
    "rollup-plugin-visualizer": "5.12.0",
    "simple-git-hooks": "2.11.1",
    "steiger": "0.5.7",
    "storybook": "8.6.14",
